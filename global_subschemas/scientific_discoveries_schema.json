{
    "$id": "https://example.com/schemas/scientific_discoveries",
    "title": "Scientific Discoveries",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "discoveryName": {
                "type": "string"
            },
            "date": {
                "type": "string",
                "format": "date"
            },
            "field": {
                "type": "string",
                "enum": [
                    "Physics",
                    "Medicine",
                    "Space",
                    "AI",
                    "Energy"
                ]
            },
            "impactLevel": {
                "type": "string",
                "enum": [
                    "Minor",
                    "Moderate",
                    "Revolutionary"
                ]
            },
            "contributingNations": {
                "type": "array",
                "items": {
                    "type": "string"
                }
            }
        },
        "required": [
            "discoveryName",
            "date",
            "field",
            "impactLevel",
            "contributingNations"
        ]
    }
}