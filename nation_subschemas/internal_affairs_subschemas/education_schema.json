{
    "$id": "https://example.com/schemas/education",
    "title": "Education",
    "type": "object",
    "properties": {
      "literacyRate": { "type": "number", "minimum": 0, "maximum": 100 },
      "educationBudget": { "type": "string", "pattern": "^\\$[0-9]+(\\.[0-9]+)?\\s?(billion|trillion)?$" },
      "topUniversities": { "type": "array", "items": { "type": "string" } },
      "STEMFundingPercentage": { "type": "number", "minimum": 0, "maximum": 100 }
    },
    "required": ["literacyRate", "educationBudget", "topUniversities", "STEMFundingPercentage"]
  }
  