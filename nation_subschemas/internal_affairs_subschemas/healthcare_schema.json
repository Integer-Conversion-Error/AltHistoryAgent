{
  "$id": "https://example.com/schemas/healthcare",
  "title": "Healthcare",
  "type": "object",
  "properties": {
    "lifeExpectancy": {
      "type": "number",
      "minimum": 0
    },
    "healthcareBudget": {
      "type": "string",
      "pattern": "^\\$[0-9]+(\\.[0-9]+)?\\s?(billion|trillion)?$"
    },
    "hospitalBedsPerThousand": {
      "type": "number",
      "minimum": 0
    },
    "publicHealthIndex": {
      "type": "number",
      "minimum": 0,
      "maximum": 100
    }
  },
  "required": [
    "lifeExpectancy",
    "healthcareBudget",
    "hospitalBedsPerThousand",
    "publicHealthIndex"
  ]
}