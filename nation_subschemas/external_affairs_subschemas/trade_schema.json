{
  "$id": "https://example.com/schemas/trade",
  "title": "Trade Agreements",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "country": {
        "type": "string"
      },
      "tradeVolume": {
        "type": "string",
        "pattern": "^\\$[0-9]+(\\.[0-9]+)?\\s?(billion|trillion)?$"
      }
    },
    "required": [
      "country",
      "tradeVolume"
    ]
  }
}