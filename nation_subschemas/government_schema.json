{
    "$id": "https://example.com/schemas/government",
    "title": "Government",
    "type": "object",
    "properties": {
      "governmentType": { "type": "string" },
      "leader": {
        "type": "object",
        "properties": {
          "name": { "type": "string" },
          "title": { "type": "string" },
          "yearsInPower": { "type": "integer", "minimum": 0 }
        },
        "required": ["name", "title", "yearsInPower"]
      },
      "politicalParties": {
        "type": "array",
        "items": { "type": "string" }
      },
      "stabilityIndex": { "type": "number", "minimum": 0, "maximum": 100 }
    },
    "required": ["governmentType", "leader", "politicalParties", "stabilityIndex"]
  }
  