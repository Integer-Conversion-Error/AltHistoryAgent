{
    "$id": "https://example.com/schemas/economic",
    "title": "Economic Policies",
    "type": "object",
    "properties": {
      "taxPolicy": {
        "type": "object",
        "properties": {
          "corporateTaxRate": { "type": "number", "minimum": 0, "maximum": 100 },
          "incomeTaxRate": { "type": "number", "minimum": 0, "maximum": 100 }
        },
        "required": ["corporateTaxRate", "incomeTaxRate"]
      },
      "industrialFocus": {
        "type": "array",
        "items": { "type": "string" }
      }
    },
    "required": ["taxPolicy", "industrialFocus"]
  }
  